{
  "name": "Browser Extension",
  "version": "1.0",
  "description": "Proxy account sharing",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js", "runtime.js"]
  },
  "permissions": ["activeTab", "webNavigation"]
}
