{
  "name": "Browser Extension",
  "version": "1.0",
  "description": "Proxy account sharing",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
      "background.js",
      "runtime.js"
    ]
  },
  "permissions": [
    "activeTab",
    "webNavigation"
  ],
  "icons": {
    "16": "assets/icon-16x16.png",
    "32": "assets/icon-32x32.png",
    "192": "assets/icon-192x192.png"
  }
}
